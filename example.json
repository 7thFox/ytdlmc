{
    "recent_uploads": 
    {
        "batch-file": [
            "https://www.youtube.com/c/videogamedunkey/videos",
            "https://www.youtube.com/c/Fatmagic/videos"
        ],
        "output": "/opt/downloads/%(uploader)s/%(uploader)s-%(title)s-%(upload_date)s.%(ext)s",
        "dateafter": "now-1days",
        "download-archive": "/opt/archive",
        "add-metadata": true,
        "write-thumbnail": true,
        "playlist-end": 10,
        "ignore-errors": true,
        "cookies": "/opt/cookies",
        "limit-rate": "2M"
    },
    "archive_all":
    {
        "batch-file": [
            "https://www.youtube.com/c/LukeSmithxyz/videos"
        ],
        "output": "/opt/downloads/%(uploader)s/%(uploader)s-%(title)s-%(upload_date)s.%(ext)s",
        "download-archive": "/opt/archive",
        "add-metadata": true,
        "write-thumbnail": true,
        "playlist-end": 10,
        "ignore-errors": true,
        "cookies": "/opt/cookies",
        "limit-rate": "2M"
    }
}